    "happy-dom": "^18.0.1",
    "nuxt": "^4.1.3",
    "oxlint": "^1.24.0",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "shadcn-nuxt": "^2.3.2",
    "tailwindcss": "^4.1.16",
